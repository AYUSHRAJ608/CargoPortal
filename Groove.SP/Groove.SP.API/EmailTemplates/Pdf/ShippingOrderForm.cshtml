@using Groove.SP.Core.Models;
@model Groove.SP.Application.POFulfillment.ViewModels.ShippingOrderFormViewModel
@{
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Noto+Sans+SC:400,700|Noto+Sans+TC:400,700|Noto+Sans:400,400i,700&subset=chinese-simplified,chinese-traditional">
    <style>
        body {
            width: 100%;
            font-family: 'Noto Sans SC', sans-serif;
            font-family: 'Noto Sans TC', sans-serif;
            font-family: 'Noto Sans', sans-serif;
            font-family: 'Microsoft Yahei';
        }

        table {
            border-collapse: collapse;
            width: 1100px;
            page-break-inside:auto;
        }

        body table:not(:first-child) tr td {
           border-top: none;
        }       

        tr {
            page-break-inside:avoid;
        }

        td, th {
            font-size: 14px;
            text-align: left;
            padding: 8px;
        }

        td {
            border: 1px solid #000;
            min-width: 35px;
        }

        div {
            min-height: 25px;
        }

        .form-title {
            font-size: 28px;
            color: #0054a6;
            text-align: right;
        }

        .title {
            font-weight: bold;
        }

        .text-right {
            text-align: right;
        }

        .big-font-size {
            font-size: 18px;
        }

        .part-break {
            text-align: center;
            font-weight: bold;
        }

        .sub-title {
            font-size: 12px;
            color: #0054a6;
        }

        .empty-box {
            padding: 20px;
        }

        .text-right {
            text-align: right;
        }

        .text-center {
            text-align: center;
        }

        .sub-content td {
            font-size: 11px;
            padding: 3px;
        }

        .extra-inform {
            font-size: 10.5px;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <td colspan="6">
                <div class="title">Shipper</div>
                @if (Model.Shipper == null && Model.ShipperOrganization == null)
                {
                    <br />
                }
                <span>@Model.Shipper?.CompanyName</span><br>
                @if (Model.ShipperOrganization != null)
                {
                    if (!string.IsNullOrEmpty(Model.ShipperOrganization?.Address))
                    {
                        <span>@Model.ShipperOrganization.Address</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.ShipperOrganization?.AddressLine2))
                    {
                        <span>@Model.ShipperOrganization.AddressLine2</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.ShipperOrganization?.AddressLine3))
                    {
                        <span>@Model.ShipperOrganization.AddressLine3</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.ShipperOrganization?.AddressLine4))
                    {
                        <span>@Model.ShipperOrganization.AddressLine4</span><br>
                    }
                    @if (Model.ShipperOrganization.Location != null)
                    {
                        <span>@Model.ShipperOrganization.Location.LocationDescription, @Model.ShipperOrganization.Location.Country.Name</span>
                    }
                }
            </td>
            <td colspan="5" class="form-title">@(Model.FormType.Equals(Groove.SP.Application.POFulfillment.ShippingFormType.Booking) ? "BOOKING FORM" : "SHIPPING ORDER FORM")</td>
        </tr>
        <tr>
            <td colspan="6">
                <div class="title">Supplier</div>
                <span>@Model.Supplier?.CompanyName</span><br>
                @if (Model.SupplierOrganization != null)
                {
                    if (!string.IsNullOrEmpty(Model.SupplierOrganization?.Address))
                    {
                        <span>@Model.SupplierOrganization.Address</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.SupplierOrganization?.AddressLine2))
                    {
                        <span>@Model.SupplierOrganization.AddressLine2</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.SupplierOrganization?.AddressLine3))
                    {
                        <span>@Model.SupplierOrganization.AddressLine3</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.SupplierOrganization?.AddressLine4))
                    {
                        <span>@Model.SupplierOrganization.AddressLine4</span><br>
                    }
                    if (Model.SupplierOrganization.Location != null)
                    {
                        <span>@Model.SupplierOrganization.Location.LocationDescription, @Model.SupplierOrganization.Location.Country.Name</span>
                    }
                }

                @if (!string.IsNullOrEmpty(@Model.Owner?.EmailAddress))
                {
                    <br><span>@Model.Owner.EmailAddress</span>
                }
                @if (!string.IsNullOrEmpty(@Model.Owner?.ContactNumber))
                {
                    <br><span>@Model.Owner.ContactNumber</span>
                }
            </td>
            <td colspan="5" class="text-right">
                <span><span class="title">SO No.</span>: @Model.SoNumber</span><br>
                <span><span class="title">@(Model.FormType.Equals(Groove.SP.Application.POFulfillment.ShippingFormType.Booking) ? "Booking Date" : "Confirm Date")</span>: @Model.ConfirmDate.ToString(Model.DateFormat)</span><br />
                <span>
                    <span class="title">Booking No.</span>: @Model.PoffNumber
                    @if (!string.IsNullOrEmpty(Model.BookingRequestReferenceNumber))
                    {
                        <br />
                        @($"({Model.BookingRequestReferenceNumber})")
                    }
                </span>
            </td>
        </tr>
        <tr>
            <td colspan="6">
                <div class="title">Consignee</div>
                @if (Model.Consignee == null && Model.ConsigneeOrganization == null)
                {
                    <br />
                }
                <span>@Model.Consignee?.CompanyName</span><br>
                @if (Model.ConsigneeOrganization != null)
                {
                    if (!string.IsNullOrEmpty(Model.ConsigneeOrganization?.Address))
                    {
                        <span>@Model.ConsigneeOrganization.Address</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.ConsigneeOrganization?.AddressLine2))
                    {
                        <span>@Model.ConsigneeOrganization.AddressLine2</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.ConsigneeOrganization?.AddressLine3))
                    {
                        <span>@Model.ConsigneeOrganization.AddressLine3</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.ConsigneeOrganization?.AddressLine4))
                    {
                        <span>@Model.ConsigneeOrganization.AddressLine4</span><br>
                    }
                    @if (Model.ConsigneeOrganization.Location != null)
                    {
                        <span>@Model.ConsigneeOrganization.Location.LocationDescription, @Model.ConsigneeOrganization.Location.Country.Name</span>
                    }
                }
            </td>
            <td colspan="5" rowspan="4" class="text-right big-font-size">
                <span class="title">@Model.OriginAgent?.CompanyName</span><br>
                @if (Model.OriginAgentOrganization != null)
                {
                    if (!string.IsNullOrEmpty(Model.OriginAgentOrganization?.Address))
                    {
                        <span>@Model.OriginAgentOrganization.Address</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.OriginAgentOrganization?.AddressLine2))
                    {
                        <span>@Model.OriginAgentOrganization.AddressLine2</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.OriginAgentOrganization?.AddressLine3))
                    {
                        <span>@Model.OriginAgentOrganization.AddressLine3</span><br>
                    }
                    if (!string.IsNullOrEmpty(Model.OriginAgentOrganization?.AddressLine4))
                    {
                        <span>@Model.OriginAgentOrganization.AddressLine4</span><br>
                    }
                    @if (Model.OriginAgentOrganization.Location != null)
                    {
                        <span>@Model.OriginAgentOrganization.Location.LocationDescription, @Model.OriginAgentOrganization.Location.Country.Name</span>
                    }
                }
            </td>
        </tr>

        <tr>
            <td colspan="6">
                <div class="title">Notify Party</div>
                @if (Model.IsNotifyPartyAsConsignee)
                {
                    <span>Same As Consignee</span>
                }
                else
                {
                    @if (Model.Notify == null && Model.NotifyOrganization == null)
                    {
                        <br />
                    }
                    @if (!string.IsNullOrEmpty(Model.Notify?.CompanyName))
                    {
                        <span>@Model.Notify.CompanyName</span><br>
                    }
                    @if (Model.NotifyOrganization != null)
                    {
                        if (!string.IsNullOrEmpty(Model.NotifyOrganization?.Address))
                        {
                            <span>@Model.NotifyOrganization.Address</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.NotifyOrganization?.AddressLine2))
                        {
                            <span>@Model.NotifyOrganization.AddressLine2</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.NotifyOrganization?.AddressLine3))
                        {
                            <span>@Model.NotifyOrganization.AddressLine3</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.NotifyOrganization?.AddressLine4))
                        {
                            <span>@Model.NotifyOrganization.AddressLine4</span><br>
                        }
                        @if (Model.NotifyOrganization.Location != null)
                        {
                            <span>@Model.NotifyOrganization.Location.LocationDescription, @Model.NotifyOrganization.Location.Country.Name</span>
                        }
                    }
                }
            </td>
        </tr>
        <tr>
            <td colspan="6">
                <div class="title">Billing Party</div>
                @if (Model.BillingAddress == null)
                {
                    <br />
                }
                @if (Model.BillingAddress != null)
                {
                    if (!string.IsNullOrEmpty(Model.BillingAddress.CompanyName))
                    {
                        <span>@(Model.BillingAddress.CompanyName)</span><br>
                    }
                    @if (Model.BillingPartyOrganization != null)
                    {
                        if (!string.IsNullOrEmpty(Model.BillingPartyOrganization?.Address))
                        {
                            <span>@Model.BillingPartyOrganization.Address</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.BillingPartyOrganization?.AddressLine2))
                        {
                            <span>@Model.BillingPartyOrganization.AddressLine2</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.BillingPartyOrganization?.AddressLine3))
                        {
                            <span>@Model.BillingPartyOrganization.AddressLine3</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.BillingPartyOrganization?.AddressLine4))
                        {
                            <span>@Model.BillingPartyOrganization.AddressLine4</span><br>
                        }
                    }
                    if (!string.IsNullOrEmpty(Model.BillingAddress.ContactDetails))
                    {
                        <span>@(Model.BillingAddress.ContactDetails)</span><br />
                    }
                    if (!string.IsNullOrEmpty(Model.BillingAddress.EmailAddress))
                    {
                        <span>@(Model.BillingAddress.EmailAddress)</span><br />
                    }
                }
            </td>
        </tr>
        <tr>
            <td colspan="6">
                <div class="title">Pickup Address</div>
                @if (Model.PickupAddress == null)
                {
                    <br />
                }
                @if (Model.PickupAddress != null)
                {
                    if (!string.IsNullOrEmpty(Model.PickupAddress.CompanyName))
                    {
                        <span>@(Model.PickupAddress.CompanyName)</span><br>
                    }
                    @if (Model.PickupOrganization != null)
                    {
                        if (!string.IsNullOrEmpty(Model.PickupOrganization?.Address))
                        {
                            <span>@Model.PickupOrganization.Address</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.PickupOrganization?.AddressLine2))
                        {
                            <span>@Model.PickupOrganization.AddressLine2</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.PickupOrganization?.AddressLine3))
                        {
                            <span>@Model.PickupOrganization.AddressLine3</span><br>
                        }
                        if (!string.IsNullOrEmpty(Model.PickupOrganization?.AddressLine4))
                        {
                            <span>@Model.PickupOrganization.AddressLine4</span><br>
                        }
                    }
                    if (!string.IsNullOrEmpty(Model.PickupAddress.ContactDetails))
                    {
                        <span>@(Model.PickupAddress.ContactDetails)</span><br />
                    }
                    if (!string.IsNullOrEmpty(Model.PickupAddress.EmailAddress))
                    {
                        <span>@(Model.PickupAddress.EmailAddress)</span><br />
                    }
                }
            </td>
        </tr>
        <tr>
            <td colspan="3">
                <div class="title">@(Model.ModeOfTransport != null && Model.ModeOfTransport.Equals(ModeOfTransport.Air, StringComparison.CurrentCultureIgnoreCase) ? "Flight No." : "Vessel/Voyage")</div>
                @if (Model.FulfillmentType == FulfillmentType.PO)
                {
                    @if (string.IsNullOrWhiteSpace(Model.FirstItinerrary?.VesselFlight ?? ""))
                    {
                        <br />
                    }
                    else
                    {
                        <span>@(Model.FirstItinerrary?.VesselFlight ?? "")</span>
                    } 
                }

                @if (Model.FulfillmentType == FulfillmentType.Bulk)
                {
                    if (Model.SOFormStage == POFulfillmentStage.ForwarderBookingRequest)
                    {
                        if (string.IsNullOrEmpty(Model.VesselName) && string.IsNullOrEmpty(Model.VoyageNo))
                        {
                            <br />
                        }
                        else
                        {
                            <span>@(Model.VesselName ?? "")/@(Model.VoyageNo ?? "")</span>
                        }
                    }

                    if (Model.SOFormStage == POFulfillmentStage.ForwarderBookingConfirmed)
                    {
                        @if (string.IsNullOrWhiteSpace(Model.FirstItinerrary?.VesselFlight ?? ""))
                        {
                            <br />
                        }
                        else
                        {
                            <span>@(Model.FirstItinerrary?.VesselFlight ?? "")</span>
                        }
                    }
                }
            </td>
            <td colspan="3">
                <div class="title">@(Model.ModeOfTransport != null && Model.ModeOfTransport.Equals(ModeOfTransport.Air, StringComparison.CurrentCultureIgnoreCase) ? "Airline" : "Carrier")</div>
                @if (Model.FulfillmentType == FulfillmentType.PO)
                {
                    @if(string.IsNullOrWhiteSpace(Model.FirstItinerrary?.CarrierName ?? ""))
                    {
                        <br />
                    }
                    else
                    {
                        <span>@(Model.FirstItinerrary?.CarrierName ?? "")</span>
                    } 
                }

                @if (Model.FulfillmentType == FulfillmentType.Bulk)
                {
                    if (Model.SOFormStage == POFulfillmentStage.ForwarderBookingRequest)
                    {
                        @if (string.IsNullOrWhiteSpace(Model.CarrierName))
                        {
                            <br />
                        }
                        else
                        {
                            <span>@(Model.CarrierName ?? "")</span>
                        }
                    }

                    if (Model.SOFormStage == POFulfillmentStage.ForwarderBookingConfirmed)
                    {
                        @if(string.IsNullOrWhiteSpace(Model.FirstItinerrary?.CarrierName ?? ""))
                        {
                            <br />
                        }
                        else
                        {
                            <span>@(Model.FirstItinerrary?.CarrierName ?? "")</span>
                        } 
                    }

                }
            </td>
            <td colspan="3" style="position:relative;">
                <span class="title">Incoterm</span>
                <span style="position:absolute; left:200px;">@(Model.Incoterm)</span>
            </td>
            <td colspan="2"></td>
        </tr>
        <tr>
            <td colspan="3">
                <div class="title">@(Model.ModeOfTransport != null && Model.ModeOfTransport.Equals(ModeOfTransport.Air, StringComparison.CurrentCultureIgnoreCase) ? "Origin" : "Port of loading")</div>
                @if (string.IsNullOrWhiteSpace(Model.PortOfLoading))
                {
                    <br />
                }
                else
                {
                    <span>@Model.PortOfLoading</span>
                } 
            </td>
            <td colspan="3">
                <div class="title">Place of receipt</div>
                @if (string.IsNullOrWhiteSpace(Model.PlaceOfReceipt))
                {
                    <br />
                }
                else
                {
                    <span>@Model.PlaceOfReceipt</span>
                } 
            </td>
            <td colspan="3" style="position:relative;">
                <span class="title">Dangerous Goods</span>&nbsp;&nbsp;&nbsp;
                <span style="position:absolute; left:200px;">
                    @if (@Model.IsDangerousGoods)
                    {
                        <img width="16" height="16" alt="checked" src="data:image/png;base64," />
                    }
                    else
                    {
                        <img width="16" height="16" alt="unchecked" src="data:image/png;base64," />
                    }
                </span>
            </td>
            <td colspan="2" style="position:relative;">
                <span class="title">CIQ or Fumigation</span>&nbsp;&nbsp;&nbsp;
                <span style="position:absolute; right:25px;">
                    @if (@Model.IsCIQOrFumigation)
                    {
                        <img width="16" height="16" alt="checked" src="data:image/png;base64," />
                    }
                    else
                    {
                        <img width="16" height="16" alt="unchecked" src="data:image/png;base64," />
                    }
                </span>
            </td>
        </tr>
        <tr>
            <td colspan="3">
                <div class="title">@(Model.ModeOfTransport != null && Model.ModeOfTransport.Equals(ModeOfTransport.Air, StringComparison.CurrentCultureIgnoreCase) ? "Destination" : "Port of discharge")</div>
                @if (string.IsNullOrWhiteSpace(Model.PortOfDischarge))
                {
                    <br />
                }
                else
                {
                    <span>@Model.PortOfDischarge</span>
                }
            </td>
            <td colspan="3">
                <div class="title">Place of delivery</div>
                @if (string.IsNullOrWhiteSpace(Model.PlaceOfDelivery))
                {
                    <br />
                }
                else
                {
                    <span>@Model.PlaceOfDelivery</span>
                }
            </td>
            <td colspan="3" style="position:relative;">
                <span class="title">Battery or Chemical</span>&nbsp;&nbsp;&nbsp;
                <span style="position:absolute; left:200px;">
                    @if (@Model.IsBatteryOrChemical)
                    {
                        <img width="16" height="16" alt="checked" src="data:image/png;base64," />
                    }
                    else
                    {
                        <img width="16" height="16" alt="unchecked" src="data:image/png;base64," />
                    }
                </span>
            </td>
            <td colspan="2" style="position:relative;">
                <span class="title">Export Licence</span>&nbsp;&nbsp;&nbsp;
                <span style="position:absolute; right:25px;">
                    @if (@Model.IsExportLicence)
                    {
                        <img width="16" height="16" alt="checked" src="data:image/png;base64," />
                    }
                    else
                    {
                        <img width="16" height="16" alt="unchecked" src="data:image/png;base64," />
                    }
                </span>
            </td>
        </tr>
        <tr>
            <td colspan="11" class="part-break">PARTICULARS FURNISHED BY VENDOR</td>
        </tr>
    </table>

    <table>
        <tr class="sub-title">
            <td class="text-center" style="width: 15%;">Shipping Marks</td>
            <td class="text-center" style="width: 15%;">Customer PO</td>
            <td class="text-center" style="width: 11%;">Product<br /><span class="extra-inform">(Inner/Outer Qty)</span></td>
            <td class="text-center" style="width: 11%;">HS Code</td>
            <td class="text-center" style="width: 8%;">No. of Cartons</td>
            <td class="text-center" style="width: 8%;">No. of Pieces</td>
            <td class="text-center" style="width: 16%;">Package Description <br />商品说明</td>
            <td class="text-center" style="width: 8%;">Volume<br /><span class="extra-inform">(CBM)</span></td>
            <td class="text-center" style="width: 8%;">Weight<br /><span class="extra-inform">(KGS)</span></td>
        </tr>

        @if (Model.Orders != null && Model.Orders.Any())
        {
            @foreach (var order in Model.Orders)
            {
                var product = Model.Products?.FirstOrDefault(x => x.PurchaseOrderId.Equals(order.PurchaseOrderId) && x.Id.Equals(order.POLineItemId));
                <tr>
                    <td class="text-center">@order.ShippingMarks</td>
                    <td class="text-center">
                        @order.CustomerPONumber
                    </td>
                    <td class="text-center">
                        @if (Model.FulfillmentType == FulfillmentType.Bulk)
                        {
                            @order.ProductCode
                        }
                        else
                        {
                            @order.ProductCode
                            <br />
                            var innerQty = order.InnerQuantity.HasValue ? order.InnerQuantity.Value.ToString("n0") : "--";
                            var outerQty = order.OuterQuantity.HasValue ? order.OuterQuantity.Value.ToString("n0") : "--";
                            @($"({innerQty}/{outerQty})")
                        }


                    </td>
                    <td class="text-right">@order.HsCode</td>
                    <td class="text-right">
                        @if (Model.FulfillmentType == FulfillmentType.Bulk)
                        {
                            @order.BookedPackage
                        }
                        else
                        {
                            @if (order.DifferentSizedCarton)
                            {
                                <span style="color:red;">* @(order.NoOfCartons.HasValue && order.NoOfCartons > 0 ? order.NoOfCartons.Value.ToString("#,##0") : "")</span>
                            }
                            else
                            {
                                <span>@(order.NoOfCartons.HasValue && order.NoOfCartons > 0 ? order.NoOfCartons.Value.ToString("#,##0") : "")</span>
                            }
                        }
                    </td>
                    <td class="text-right">@order.NoOfPieces.ToString("#,##0")</td>
                    <td>
                        @(Model.FulfillmentType == FulfillmentType.Bulk ? order?.ProductName : product?.DescriptionOfGoods)
                        @if (!string.IsNullOrEmpty(order.ChineseDescription))
                        {
                            <br />
                            @order.ChineseDescription
                        }
                    </td>
                    <td class="text-right">@(order.Volume.HasValue && order.Volume > 0 ? order.Volume.Value.ToString("#,##0.##0") : "")</td>
                    <td class="text-right">@(order.Weight.HasValue && order.Weight > 0 ? order.Weight.Value.ToString("#,##0.#0") : "")</td>
                </tr>
            }
        }

        <tr>
            <td colspan="9"></td>
        </tr>
        <tr>
            <td colspan="4" class="title text-right">Total</td>
            @{
                var subOrderTotal = Model.Orders.Where(x => x.NoOfCartons.HasValue && x.NoOfCartons > 0);
            }
            <td class="text-right">@(subOrderTotal != null && subOrderTotal.Any() ? subOrderTotal.Sum(x=>x.NoOfCartons).Value.ToString("#,##0") : "")</td>

            <td class="text-right">@Model.Orders.Sum(x => x.NoOfPieces).ToString("#,##0")</td>
            <td></td>

            @{
                subOrderTotal = Model.Orders.Where(x => x.Volume.HasValue && x.Volume > 0);
            }
            <td class="text-right">@(subOrderTotal != null && subOrderTotal.Any() ? subOrderTotal.Sum(x=>x.Volume).Value.ToString("#,##0.##0") : "")</td>

            @{
                subOrderTotal = Model.Orders.Where(x => x.Weight.HasValue && x.Weight > 0);
            }
            <td class="text-right">@(subOrderTotal != null && subOrderTotal.Any() ? subOrderTotal.Sum(x=>x.Weight).Value.ToString("#,##0.#0") : "")</td>
        </tr>

    </table>

    <table>
        <tr>
            <td colspan="11"></td>
        </tr>
        <tr>
            
            @if(Model.ModeOfTransport != null && Model.ModeOfTransport.Equals(ModeOfTransport.Air, StringComparison.CurrentCultureIgnoreCase))
            {
            <td colspan="6">
                <span><span class="title">Expected Ship Date</span>: @(Model.ExpectedShipDate?.ToString(Model.DateFormat) ?? "")</span>
            </td>
            <td colspan="5">
                <span>
                    <span class="title">Cargo Ready Date</span>: @(Model.FulfillmentType == FulfillmentType.Bulk ? Model.CargoReadyDate?.ToString(Model.DateFormat)?? "" : Model.PoffShipmentDate?.ToString(Model.DateFormat) ?? "")
                </span>
            </td>
            }
            else
            {
            <td colspan="4">
                <span><span class="title">Expected Ship Date</span>: @(Model.ExpectedShipDate?.ToString(Model.DateFormat) ?? "")</span>
            </td>
            <td colspan="4">
                <span>
                    <span class="title">Cargo Ready Date</span>: @(Model.FulfillmentType == FulfillmentType.Bulk ? Model.CargoReadyDate?.ToString(Model.DateFormat)?? "" : Model.PoffShipmentDate?.ToString(Model.DateFormat) ?? "")
                </span>
            </td>
            <td colspan="3">
                <span><span class="title">Container</span>: @Model.BookingContainers</span>
            </td>
            }
        </tr>
        <tr>
            <td colspan="11">
                <span style="white-space: pre-line"><span class="title">Remarks</span>: @(Model.Remarks)</span>
            </td>
        </tr>
        <tr class="sub-content">
            <td colspan="11">THIS DOCK RECEIPT INCLUDE CARGO RECEIVED ON WAREHOUSE RECEIVING RECORDS NUMBERED AS FOLLOWING:</td>
        </tr>
        <tr class="sub-content">
            <td colspan="11">TOTAL NUMBER OF PACKAGES IN WRITING:</td>
        </tr>
        <tr class="sub-content">
            <td colspan="11">EXCEPTION WERE NOTED AS FOLLOWS:</td>
